{
	"name": "voice-bot",
	"version": "2.0.0",
	"description": "Vapi Cold Call Testing Bot - SDR Training with Joey Gilkey personas",
	"main": "dist/cli.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"dev": "tsx watch src/cli.ts",
		"vapi": "op run --env-file=.env -- tsx src/cli.ts",
		"vapi:list": "op run --env-file=.env -- tsx src/cli.ts list",
		"vapi:phones": "op run --env-file=.env -- tsx src/cli.ts phones",
		"vapi:calls": "op run --env-file=.env -- tsx src/cli.ts calls",
		"vapi:call": "op run --env-file=.env -- tsx src/cli.ts call",
		"vapi:webhook": "op run --env-file=.env -- tsx src/cli.ts webhook",
		"vapi:sync": "op run --env-file=.env -- tsx src/cli.ts sync",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"bin": {
		"vapi-bot": "./dist/cli.js"
	},
	"keywords": [
		"vapi",
		"voice-ai",
		"cold-calling",
		"sdr-training",
		"sales",
		"ai-training"
	],
	"author": "jesse@leadmagic.io",
	"license": "ISC",
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@types/react": "19.2.10",
		"@vapi-ai/server-sdk": "0.11.0",
		"@vapi-ai/web": "2.5.2",
		"boxen": "8.0.1",
		"chalk": "5.6.2",
		"cli-table3": "0.6.5",
		"dotenv": "17.2.3",
		"figures": "6.1.0",
		"ink": "6.6.0",
		"ink-spinner": "5.0.0",
		"react": "19.2.4"
	},
	"devDependencies": {
		"@types/node": "25.1.0",
		"tsx": "4.21.0",
		"typescript": "5.9.3"
	}
}
